{
  "name": "jupiter-notify",
  "version": "3.1.3",
  "description": "Custom dashboard for monthly subscriptions.",
  "main": "server.js",
  "scripts": {
    "start": "nodemon node src/server.js",
    "remote": "concurrently \"lt --port 8080 --subdomain jupiternotify\" \"SET NODE_ENV=production&& node src/server.js\"",
    "production": "node src/server.js",
    "lint": "eslint **/*.js --ignore-pattern node_modules/",
    "commit": "git-cz",
    "prerelease": "git checkout master && npm i && npm run lint && git add .",
    "release": "standard-version -a",
    "postrelease": "run-s release:*",
    "release:tags": "git push --follow-tags origin master",
    "release:github": "conventional-github-releaser -p angular",
    "deploy": "git push --follow-tags heroku master"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "engines": {
    "node": "14.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcelmiro/jupiter-notify.git"
  },
  "author": "Marcel Miro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marcelmiro/jupiter-notify/issues"
  },
  "homepage": "https://github.com/marcelmiro/jupiter-notify#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "browser-detect": "^0.2.28",
    "compression": "^1.7.4",
    "cookie-session": "^1.4.0",
    "discord.js": "^12.3.1",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.3",
    "helmet": "^4.1.1",
    "http": "0.0.1-security",
    "https": "^1.0.0",
    "joi": "^17.2.1",
    "passport": "^0.4.1",
    "passport-discord": "^0.1.4",
    "path": "^0.12.7",
    "pg": "^8.3.3",
    "simple-node-logger": "^18.12.24",
    "socket.io": "^2.3.0",
    "stripe": "^8.96.0",
    "uuid": "^8.3.0",
    "vue": "^2.6.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@commitlint/prompt": "^11.0.0",
    "babel-eslint": "^10.1.0",
    "commitizen": "^4.2.1",
    "concurrently": "^5.3.0",
    "conventional-github-releaser": "^3.1.5",
    "eslint": "^7.9.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "localtunnel": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "standard-version": "^9.0.0"
  }
}
